	<sqlMap resource="/sql/checkInfoSQL.xml"/> <!-- 유효성검사 : 아이디검사, 비밀번호 검사 -->
	<sqlMap resource="/sql/FranchiseeManageSQL.xml"/>  <!--  -->
</sqlMapConfig>